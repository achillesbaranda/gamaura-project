{
  "buildCommand": "bash build.sh",
  "outputDirectory": "build",
  "installCommand": "npm install --legacy-peer-deps",
  "devCommand": "npm start",
  "framework": null,
  "routes": [
    {
      "src": "/static/(.*)",
      "headers": {
        "cache-control": "s-maxage=31536000,immutable"
      }
    },
    {
      "src": "/(.*)",
      "dest": "/index.html"
    }
  ]
}